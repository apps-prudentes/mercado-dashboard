{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ],
    "env": {
        "APP_ID": "@app_id",
        "APP_SECRET": "@app_secret",
        "REDIRECT_URI": "https://omargaxiola.com/api/callback",
        "PORT": "3000"
    }
}